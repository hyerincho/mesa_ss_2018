&constants
/

&model
	model_type = 'EVOL'
	file = 'fgong.l5bi.d.15c'
	file_format = 'FGONG'
/

&mode
	l = 1000
/

&osc
	outer_bound = 'JCD'
/

&num
	diff_scheme = 'COLLOC_GL6'
/

&scan
	grid_type = 'LINEAR'
	freq_min = 3000
	freq_max = 5000
	freq_min_units = 'UHZ'
	freq_max_units = 'UHZ'
	n_freq = 10
/

&grid
	alpha_osc = 5.
	alpha_exp = 1.
/

&ad_output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,freq'
	freq_units = 'UHZ'
/

&nad_output
/
