&constants
/

&model
	model_type = 'POLY'
        file = 'poly.h5'
/

&mode
	l = 1
	tag = 'GL2'
/

&mode
	l = 1
	tag = 'GL4'
/

&mode
	l = 1
	tag = 'GL6'
/

&osc
/

&num
	diff_scheme = 'COLLOC_GL2'
	tag_list = 'GL2'
/

&num
	diff_scheme = 'COLLOC_GL4'
	tag_list = 'GL4'
/

&num
	diff_scheme = 'COLLOC_GL6'
	tag_list = 'GL6'
/

&scan
	grid_type = 'INVERSE'
	freq_min = 0.3
	freq_max = 1.0
	n_freq = 50
/

&scan
	grid_type = 'LINEAR'
	freq_min = 1.0
	freq_max = 10.0
	n_freq = 50
/

&grid
/

&ad_output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,m,n_pg,n_p,n_g,omega'
/

&nad_output
/
