&constants
/

&model
	model_type = 'EVOL'
	file = 'bcep.mesa'
	file_format = 'MESA'
/

&mode
	l = 0
/

&mode
	l = 1
/

&mode
	l = 2
/

&mode
	l = 3
/

&osc
	nonadiabatic = .TRUE.
/

&num
	diff_scheme = 'MAGNUS_GL2'
	restrict_roots = .FALSE.
/

&scan
	grid_type = 'LINEAR'
	freq_min = 3.0
	freq_max = 10.0
	n_freq = 50
/

&grid
	alpha_osc = 10
	alpha_exp = 2
	n_inner = 5
/

&ad_output
/

&nad_output
	summary_file = 'summary.txt'
	summary_file_format = 'TXT'
	summary_item_list = 'l,n_pg,n_p,n_g,omega,omega_int'
/
